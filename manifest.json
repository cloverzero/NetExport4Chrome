{
  "manifest_version": 2,

  "name": "NetExport4Chrome",
  "description": "NetExport For Chrome",
  "version": "1.0",

  "devtools_page": "devtools.html",

  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },

  "permissions": [
    "tabs",
    "downloads",
    "alarms",

    "http://*/*"
  ]
}